BB_USER[0]="bbslave"                      # user to run as
BB_BASEDIR[0]="/home/bbslave/buildbot"    # basedir argument to buildbot (absolute path)
BB_OPTIONS[0]=""                          # buildbot options
BB_PREFIXCMD[0]=""                        # prefix command, i.e. nice, linux32, dchroot
